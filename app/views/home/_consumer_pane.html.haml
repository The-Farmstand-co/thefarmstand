- content_for :injection_data do
  - cache(*CacheService::FragmentCaching.ams_shops) do
    = inject_enterprises(@enterprises)

#consumer.pane
  .row
    .small-6.text-left.columns
      %h2
        = t :consumer_pane_headline
      %p
        = t :consumer_pane_body
    .small-6.text-left.columns
      %div.bullet
        %img{src: "", width: "50"}
        = t :consumer_pane_bullet1
      %div.bullet
        %img{src: "", width: "50"}
        = t :consumer_pane_bullet2
      %div.bullet
        %img{src: "", width: "50"}
        = t :consumer_pane_bullet3
    .small-12.text-center.columns{"ng-controller" => "EnterprisesCtrl", "ng-cloak" => true}
      %p
        = t :consumer_pane_action_prompt
      = render "shared/components/enterprise_search"