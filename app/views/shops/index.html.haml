- content_for(:title) do
  = t :shops_title

- content_for :injection_data do
  - cache(*CacheService::FragmentCaching.ams_shops) do
    = inject_enterprises(@enterprises)

#panes
  #shops.pane
    .row
      .small-12.medium-10.medium-offset-1.columns.text-center
        %h2
          = t :shops_headline
      .small-12.columns.text-center
        = form_tag (shops_path, :method => :get) do
          = text_field_tag (:search, params[:search], placeholder: "Enter your Zipcode, Farm, or Product" )
          = submit_tag("Search", :name => nil)

/ = render "hubs"
= render "shared/footer-redesign"
